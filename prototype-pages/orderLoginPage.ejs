<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Order Login Page</title>
</head>
<body>
  <h1>Welcome, Order Processor.</h1>
  <form id="order-login" action="">
    <input type="text" name="username" id="username" placeholder="User ID">
    <input type="password" name="password" id="password" placeholder="Password">
    <button name="loginButton" type="submit">Login</button>
  </form>
  <script>
    const loginForm = document.getElementById("order-login");

    loginForm.loginButton.addEventListener("click", (e) => {
      e.preventDefault();
      window.location = "/NewOrdersPage.html";
    });
  </script>
</body>
</html>
